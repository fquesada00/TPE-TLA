graph(){
    int t0 = 0;
    int t1 = 1;
    int next = t0 + t1;
    output("Fibonacci sequence for ten first numbers\n\n");
    for(int i = 0; i < 10 ; i = i + 1) {
        t0 = t1;
        t1 = next;
        next = t0 + t1;
        output("Fibonacci number for this iteration:\n");
        output(next);
        output("\n");
    }
}